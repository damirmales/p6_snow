{% extends 'base.html.twig' %}

{% block title %}Ajout de médias{% endblock %}

{% block body %}
    {{ form_start(form) }}
    <div class="container mt-4">
        <p>Ajout de médias pour la figure {{ figTitle }}</p>
        {{ form_widget(form) }}
        <div class="row">
            <button type="submit" class="btn btn-primary">Soumettre</button>
        </div>
    </div>
    {{ form_end(form) }}

{% endblock %}